{
  "name": "toa-react",
  "author": "Kyle Flynn",
  "license": "MIT",
  "version": "0.0.1",
  "description": "Progressive Web Application for The Orange Alliance",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "preserve": "webpack --config ./config/webpack.library.config.js",
    "serve": "webpack-dev-server --config ./config/webpack.client.dev.config.js --open",
    "build-client": "webpack -p --config ./config/webpack.client.prod.config.js",
    "build-server": "webpack -p --config ./config/webpack.server.config.js",
    "build": "npm run build-client && npm run build-server",
    "start": "nodemon build/index.js --config nodemon.json",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@the-orange-alliance/api": "0.0.7",
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "https-proxy-agent": "^5.0.0",
    "i18next": "^19.7.0",
    "i18next-chained-backend": "^2.0.1",
    "i18next-fs-backend": "^1.0.7",
    "i18next-http-backend": "^1.0.18",
    "i18next-http-middleware": "^3.0.2",
    "moment": "^2.27.0",
    "net": "^1.0.2",
    "react": "^16.13.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "redux": "^4.0.5",
    "remote-redux-devtools": "^0.5.16",
    "tls": "0.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/remote-redux-devtools": "^0.5.4",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "@types/react-router-dom": "^5.1.5",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^5.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.2.1",
    "dotenv": "^8.2.0",
    "editorconfig": "^0.15.3",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "ignore-loader": "^0.1.2",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "sass-loader": "^9.0.3",
    "source-map-loader": "^1.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "ts-loader": "^8.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
