    "@material-ui/lab": "^4.0.0-alpha.50",
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
